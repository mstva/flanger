<% if signed_in? %>

    <h1>Home#index</h1>
    <p>Find me in app/views/home/index.html.erb</p>

    Signed in as: <%= current_user.email %>
    <%= button_to 'Logout', logout_path, method: :delete %>

<% else %>
      <%= link_to 'Login', login_path %>
      <%= link_to 'Register', register_path %>
<% end %>
